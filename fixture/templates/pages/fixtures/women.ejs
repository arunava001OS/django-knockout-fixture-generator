    <%- contentFor('scripts') %>
        <script src="/js/jquery.brackets.min.js"></script>

            <script>
    
var fixtureData = <%-JSON.stringify(fixture) %>;
 
$(function() {
    $('#fixture').bracket({
    teamWidth: 200,
  scoreWidth: 40,
  matchMargin: 50,
  roundMargin: 50,
  skipConsolationRound: false,
  init: fixtureData,
  centerConnectors:true
     });
  })
    </script>

